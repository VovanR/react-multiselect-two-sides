{
  "name": "react-multiselect-two-sides",
  "version": "0.11.0",
  "description": "React multiselect two sides component",
  "license": "MIT",
  "repository": "VovanR/react-multiselect-two-sides",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "build": "webpack",
    "start": "webpack --watch",
    "test": "eslint index.jsx test/*.js test/**/*.js webpack.config.js example/app.jsx && ava --verbose",
    "precommit": "npm test",
    "tdd": "ava --watch",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "deploy": "gh-pages-deploy"
  },
  "files": [
    "index.jsx",
    "style.css"
  ],
  "main": "index.jsx",
  "style": "style.css",
  "keywords": [
    "react",
    "component",
    "react-component",
    "select",
    "multiselect",
    "input"
  ],
  "dependencies": {
    "classnames": "^2.2.5"
  },
  "devDependencies": {
    "autoprefixer": "^6.7.3",
    "ava": "^0.18.1",
    "babel-core": "^6.23.1",
    "babel-loader": "^6.3.2",
    "babel-preset-modern-browsers": "^8.0.0",
    "babel-preset-react": "^6.23.0",
    "babel-register": "^6.23.0",
    "conventional-changelog-cli": "^1.2.0",
    "css-loader": "^0.26.1",
    "cz-conventional-changelog": "^1.2.0",
    "enzyme": "^2.7.1",
    "eslint": "^3.15.0",
    "eslint-config-xo": "^0.17.0",
    "eslint-config-xo-react": "^0.10.0",
    "eslint-plugin-react": "^6.10.0",
    "eslint-plugin-xo": "^1.0.0",
    "husky": "^0.13.1",
    "jsdom": "^9.11.0",
    "mversion": "^1.10.1",
    "postcss-loader": "^1.3.0",
    "precss": "^1.4.0",
    "react": "^15.4.2",
    "react-addons-test-utils": "^15.4.2",
    "react-dom": "^15.4.2",
    "style-loader": "^0.13.1",
    "webpack": "^2.2.1"
  },
  "peerDependencies": {
    "react": "^0.14.0 || ^15.0.0",
    "react-dom": "^0.14.0 || ^15.0.0"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "gh-pages-deploy": {
    "staticpath": "example",
    "prep": [
      "build"
    ],
    "noprompt": false
  }
}
