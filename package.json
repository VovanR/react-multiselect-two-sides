{
  "name": "react-multiselect-two-sides",
  "version": "0.16.0",
  "description": "React multiselect two sides component",
  "license": "MIT",
  "repository": "VovanR/react-multiselect-two-sides",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:example": "cross-env NODE_ENV=production webpack -p",
    "start": "webpack --watch",
    "test": "npm run lint && ava --verbose",
    "lint": "eslint src/* test/*.js test/**/*.js webpack.config.js example/app.jsx",
    "tdd": "ava --watch",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "deploy": "npm run build:example && gh-pages -d example",
    "prepublishOnly": "del-cli lib && npm run build"
  },
  "files": [
    "lib",
    "style.css"
  ],
  "main": "lib/index.js",
  "style": "style.css",
  "keywords": [
    "react",
    "component",
    "react-component",
    "select",
    "multiselect",
    "input"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.7.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.4",
    "@babel/core": "^7.4.5",
    "@babel/preset-env": "^7.4.5",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.4.4",
    "autoprefixer": "^9.5.1",
    "ava": "1.4.1",
    "babel-loader": "^8.0.6",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.24",
    "conventional-changelog-cli": "^2.0.21",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.1",
    "eslint": "^5.16.0",
    "eslint-config-xo": "^0.26.0",
    "eslint-config-xo-react": "^0.19.0",
    "eslint-plugin-react": "^7.13.0",
    "eslint-plugin-react-hooks": "^1.6.0",
    "eslint-plugin-xo": "^1.0.0",
    "gh-pages": "^2.0.1",
    "husky": "^2.3.0",
    "jsdom": "^15.1.0",
    "mversion": "^2.0.0",
    "postcss-loader": "^3.0.0",
    "precss": "^4.0.0",
    "raf": "^3.4.1",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "style-loader": "^0.23.1",
    "webpack": "^4.32.2",
    "webpack-cli": "^3.3.2"
  },
  "peerDependencies": {
    "classnames": "*",
    "prop-types": "*",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "pre-commit": "npm test"
  }
}
