{
  "name": "react-multiselect-two-sides",
  "version": "0.14.0",
  "description": "React multiselect two sides component",
  "license": "MIT",
  "repository": "VovanR/react-multiselect-two-sides",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:example": "cross-env NODE_ENV=production webpack -p",
    "start": "webpack --watch",
    "test": "npm run lint && ava --verbose",
    "lint": "eslint src/* test/*.js test/**/*.js webpack.config.js example/app.jsx",
    "precommit": "npm test",
    "tdd": "ava --watch",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "deploy": "npm run build:example && gh-pages -d example",
    "prepublishOnly": "del-cli lib && npm run build"
  },
  "files": [
    "lib",
    "style.css"
  ],
  "main": "lib/index.js",
  "style": "style.css",
  "keywords": [
    "react",
    "component",
    "react-component",
    "select",
    "multiselect",
    "input"
  ],
  "dependencies": {
    "classnames": "^2.2.6",
    "prop-types": "^15.6.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.0.1",
    "@babel/preset-env": "^7.0.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/register": "^7.0.0",
    "autoprefixer": "^9.1.5",
    "ava": "1.0.0-beta.8",
    "babel-loader": "^8.0.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.15",
    "conventional-changelog-cli": "^2.0.5",
    "cross-env": "^5.2.0",
    "css-loader": "^1.0.0",
    "cz-conventional-changelog": "^2.1.0",
    "del-cli": "^1.1.0",
    "enzyme": "^3.6.0",
    "enzyme-adapter-react-16": "^1.5.0",
    "eslint": "^5.6.0",
    "eslint-config-xo": "^0.25.0",
    "eslint-config-xo-react": "^0.17.0",
    "eslint-plugin-react": "^7.11.1",
    "eslint-plugin-xo": "^1.0.0",
    "gh-pages": "^2.0.0",
    "husky": "^0.14.3",
    "jsdom": "^12.0.0",
    "mversion": "^1.12.0",
    "postcss-loader": "^3.0.0",
    "precss": "^3.1.2",
    "raf": "^3.4.0",
    "react": "^16.5.1",
    "react-dom": "^16.5.1",
    "style-loader": "^0.23.0",
    "webpack": "^4.19.0",
    "webpack-cli": "^3.1.0"
  },
  "peerDependencies": {
    "classnames": "*",
    "prop-types": "*",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "ava": {
    "require": [
      "@babel/register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
