{
  "name": "react-multiselect-two-sides",
  "version": "0.13.2",
  "description": "React multiselect two sides component",
  "license": "MIT",
  "repository": "VovanR/react-multiselect-two-sides",
  "author": {
    "name": "Vladimir Rodkin",
    "email": "mail@vovanr.com",
    "url": "https://github.com/VovanR"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production babel src --out-dir lib",
    "build:example": "cross-env NODE_ENV=production webpack -p",
    "start": "webpack --watch",
    "test": "npm run lint && ava --verbose",
    "lint": "eslint src/* test/*.js test/**/*.js webpack.config.js example/app.jsx",
    "precommit": "npm test",
    "tdd": "ava --watch",
    "release-patch": "mversion patch",
    "release-minor": "mversion minor",
    "release-major": "mversion major",
    "deploy": "npm run build:example && gh-pages -d example",
    "prepublishOnly": "del-cli lib && npm run build"
  },
  "files": [
    "lib",
    "style.css"
  ],
  "main": "lib/index.js",
  "style": "style.css",
  "keywords": [
    "react",
    "component",
    "react-component",
    "select",
    "multiselect",
    "input"
  ],
  "dependencies": {
    "classnames": "^2.2.5",
    "prop-types": "^15.6.0"
  },
  "devDependencies": {
    "autoprefixer": "^7.1.6",
    "ava": "^0.23.0",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-transform-react-remove-prop-types": "^0.4.12",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-register": "^6.26.0",
    "conventional-changelog-cli": "^1.3.4",
    "cross-env": "^5.1.3",
    "css-loader": "^0.28.7",
    "cz-conventional-changelog": "^2.0.0",
    "del-cli": "^1.1.0",
    "enzyme": "^3.1.0",
    "enzyme-adapter-react-16": "^1.0.2",
    "eslint": "^4.9.0",
    "eslint-config-xo": "^0.19.0",
    "eslint-config-xo-react": "^0.13.0",
    "eslint-plugin-react": "^7.4.0",
    "eslint-plugin-xo": "^1.0.0",
    "gh-pages": "^1.1.0",
    "husky": "^0.14.3",
    "jsdom": "^11.3.0",
    "mversion": "^1.10.1",
    "postcss-loader": "^2.0.8",
    "precss": "^2.0.0",
    "raf": "^3.4.0",
    "react": "^16.0.0",
    "react-dom": "^16.0.0",
    "style-loader": "^0.19.0",
    "webpack": "^3.8.1"
  },
  "peerDependencies": {
    "classnames": "*",
    "prop-types": "*",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"
  },
  "ava": {
    "babel": "inherit",
    "require": [
      "babel-register",
      "./test/helpers/setup-test-env.js"
    ]
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
